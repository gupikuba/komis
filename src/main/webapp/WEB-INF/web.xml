<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>servletKuba</servlet-name>
    <servlet-class>pl.altkom.web.ServletKuba</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>controllerServlet</servlet-name>
    <servlet-class>pl.altkom.web.ControllerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>redirectServlett</servlet-name>
    <servlet-class>pl.altkom.web.RedirectServlett</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>servletKuba</servlet-name>
    <url-pattern>/hellokuba</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>controllerServlet</servlet-name>
    <url-pattern>/controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>redirectServlett</servlet-name>
    <url-pattern>/redirectServlett</url-pattern>
  </servlet-mapping>

</web-app>
